    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.21.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "lucide-react": "^0.475.0",
    "openai": "^4.0.0",
    "react-chartjs-2": "^5.3.0",
    "recharts": "^2.15.1"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "concurrently": "^9.1.2",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
